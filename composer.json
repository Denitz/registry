{
    "name": "joomla/keychain",
    "type": "joomla-package",
    "description": "Joomla Keychain Package",
    "keywords": ["joomla", "framework", "keychain"],
    "homepage": "https://github.com/joomla-framework/keychain",
    "license": "LGPL-2.1+",
    "require": {
        "php": "~7.0",
        "joomla/crypt": "~2.0",
        "joomla/registry": "^1.4.5|~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.3",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Keychain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Keychain\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    },
    "bin": ["bin/keychain"]
}
