{
    "name": "joomla/crypt",
    "type": "joomla-package",
    "description": "Joomla Crypt Package",
    "keywords": ["joomla", "framework", "crypt"],
    "homepage": "https://github.com/joomla-framework/crypt",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10"
    },
    "require-dev": {
        "joomla/test": "~1.0",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Crypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Crypt\\Test\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
