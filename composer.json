{
    "name": "joomla/oauth2",
    "type": "joomla-package",
    "description": "Joomla OAuth2 Package",
    "keywords": [
        "joomla",
        "framework",
        "oauth2"
    ],
    "homepage": "https://github.com/joomla-framework/oauth2",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^7.2.5|~8.0.0|~8.1.0",
        "joomla/application": "dev-php-8.2",
        "joomla/http": "dev-php-8.2",
        "joomla/input": "dev-php-8.2",
        "joomla/session": "dev-php-8.2",
        "joomla/uri": "dev-php-8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\OAuth2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\OAuth2\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
