{
    "name": "joomla/language",
    "type": "joomla-package",
    "description": "Joomla Language Package",
    "keywords": ["joomla", "framework", "language"],
    "homepage": "https://github.com/joomla-framework/language",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~7.2",
        "ext-xml": "*",
        "joomla/string": "~1.3|~2.0"
    },
    "require-dev": {
        "joomla/coding-standards": "~2.0@alpha",
        "joomla/di": "~1.0|~2.0",
        "joomla/registry": "^1.4.5|~2.0",
        "joomla/test": "~2.0",
        "phpunit/phpunit": "~8.2"
    },
    "suggest": {
        "joomla/di": "To use the Language ServiceProviderInterface objects, install joomla/di."
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Language\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Language\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
