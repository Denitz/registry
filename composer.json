{
    "name": "joomla/oauth1",
    "type": "joomla-package",
    "description": "Joomla OAuth1 Package",
    "keywords": [
        "joomla",
        "framework",
        "oauth1"
    ],
    "homepage": "https://github.com/joomla-framework/oauth1",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^7.2.5|~8.0.0|~8.1.0",
        "joomla/application": "2.0-dev",
        "joomla/http": "2.0-dev",
        "joomla/input": "2.0-dev",
        "joomla/registry": "2.0-dev",
        "joomla/session": "2.0-dev",
        "joomla/uri": "2.0-dev"
    },
    "require-dev": {
        "joomla/event": "2.0-dev",
        "joomla/test": "2.0-dev",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\OAuth1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\OAuth1\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
